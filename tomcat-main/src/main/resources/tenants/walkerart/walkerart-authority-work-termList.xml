<record id="preferredWork">
    <section id="termInformation">
        <repeat id="workTermGroupList/workTermGroup">
            <field id="termLanguage" ui-search="repeatable" autocomplete="vocab-languages" ui-type="enum" seperate_ui_container="true">
                <selector>workAuthority-termLanguage</selector>
                 <!-- changing URN for default language value -->
                <enum>
                    <default>urn:cspace:walkerart.org:vocabularies:name(languages):item:name(eng)'English'</default>
                </enum>
            </field>
        </repeat>
    </section>
</record>
